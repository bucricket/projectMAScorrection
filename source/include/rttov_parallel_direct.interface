Interface
SUBROUTINE rttov_parallel_direct( &
      &   errorstatus      &
      & , chanprof         &
      & , opts             &
      & , profiles         &
      & , coefs            &
      & , transmission     &
      & , radiancedata     &
      & , radiancedata2    &
      & , calcemis         &
      & , emissivity       &
      & , calcrefl         &
      & , reflectance      &
      & , aer_opt_param    &
      & , cld_opt_param    &
      & , traj             &
      & , pccomp           &
      & , channels_rec     &
      & , nthreads         &
      & , strategy         &
      & , debug)
  USE rttov_types, ONLY:        &
        & rttov_options,        &
        & rttov_chanprof,       &
        & rttov_emissivity,     &
        & rttov_reflectance,    &
        & rttov_opt_param,      &
        & rttov_pccomp,         &
        & rttov_coefs,          &
        & profile_type,         &
        & transmission_type,    &
        & radiance_type,        &
        & radiance2_type,       &
        & rttov_traj
  USE parkind1, ONLY:jpim, jplm
  IMPLICIT NONE
  INTEGER(KIND=jpim),             INTENT(OUT)   :: errorstatus ! return flag
  TYPE(rttov_chanprof),           INTENT(IN)    :: chanprof(:)
  TYPE(rttov_options),            INTENT(IN)    :: opts
  TYPE(profile_type),             INTENT(IN)    :: profiles(:) ! Atmospheric profiles
  TYPE(rttov_coefs),              INTENT(IN)    :: coefs
  TYPE(transmission_type),        INTENT(INOUT) :: transmission     ! transmittances and layer optical depths
  TYPE(radiance_type),            INTENT(INOUT) :: radiancedata     ! radiances (mw/cm-1/ster/sq.m) and degK
  TYPE(radiance2_type), OPTIONAL, INTENT(INOUT) :: radiancedata2    ! secondary radiances
  LOGICAL(KIND=jplm),      OPTIONAL, TARGET, INTENT(IN)    :: calcemis(:)    ! switch for emissivity calc.
  TYPE(rttov_emissivity),  OPTIONAL, TARGET, INTENT(INOUT) :: emissivity(:)  ! surface emissivity
  LOGICAL(KIND=jplm),      OPTIONAL, TARGET, INTENT(IN)    :: calcrefl(:)    ! switch for reflectance calc.
  TYPE(rttov_reflectance), OPTIONAL, TARGET, INTENT(INOUT) :: reflectance(:) ! surface reflectance
  TYPE(rttov_opt_param), OPTIONAL, INTENT(IN)  :: aer_opt_param
  TYPE(rttov_opt_param), OPTIONAL, INTENT(IN)  :: cld_opt_param
  TYPE(rttov_traj),    OPTIONAL, INTENT(INOUT) :: traj
  TYPE(rttov_pccomp),  OPTIONAL, INTENT(INOUT) :: pccomp
  INTEGER(KIND=jpim),  OPTIONAL, INTENT(IN)    :: channels_rec(:)
  INTEGER(KIND=jpim),  OPTIONAL, INTENT(IN)    :: nthreads
  INTEGER(KIND=jpim),  OPTIONAL, INTENT(IN)    :: strategy
  LOGICAL(KIND=jplm),  OPTIONAL, INTENT(IN)    :: debug
End Subroutine
End Interface
